{
  "name": "task-manager",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prestart": "npm run build",
    "start": "node dist/src/app.js",
    "preserve": "npm run build",
    "serve": "tsc -w & nodemon dist/src/app.js",
    "pretest": "npm run build",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --detectOpenHandles --forceExit",
    "dev": "ts-node src/app.ts"
  },
  "dependencies": {
    "@arcjet/node": "^1.0.0-beta.16",
    "@upstash/workflow": "^0.2.23",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.2",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-typescript": "^7.28.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
